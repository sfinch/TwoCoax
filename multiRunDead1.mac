# Macro File

#turn off visulizer
/vis/disable
#only print ever 10000 events
/TwoCoax/event/printModulo 10000
#set energy
/gps/energy 661.657 keV
#set detector distance
/TwoCoax/det/setDetectorDistance 24.9 cm
#set detector distance
/TwoCoax/gun/rndm on
